# Конфигурация сервера BungaaCord Desktop
# Скопируйте этот файл в .env.test для запуска npm start и .env.prod для билда версии production

# URL сервера BungaaCord (если отличается от localhost:8080)
SERVER_URL=http://localhost:8080

# Игнорировать проверку SSL сертификатов (для самоподписанных сертификатов)
IGNORE_SSL=false

# Настройки автозапуска приложения
AUTO_START=false

# Настройки обновления
AUTO_UPDATE=true

# Настройки логирования
LOG_LEVEL=info

# Настройки окна
WINDOW_WIDTH=1200
WINDOW_HEIGHT=800
MIN_WIDTH=800
MIN_HEIGHT=600